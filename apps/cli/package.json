{
  "name": "@vena/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Vena AI Agent Platform - CLI",
  "bin": {
    "vena": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@vena/shared": "workspace:*",
    "@vena/providers": "workspace:*",
    "@vena/core": "workspace:*",
    "@vena/gateway": "workspace:*",
    "@vena/channels": "workspace:*",
    "commander": "^13.1.0",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "typescript": "~5.7.3",
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9"
  }
}
