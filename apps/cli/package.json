{
  "name": "@vena/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Vena AI Agent Platform - CLI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Codevena/Vena.git",
    "directory": "apps/cli"
  },
  "keywords": ["vena", "ai", "agent", "cli"],
  "files": ["dist"],
  "bin": {
    "vena": "./dist/bootstrap.js"
  },
  "main": "./dist/bootstrap.js",
  "types": "./dist/bootstrap.d.ts",
  "exports": {
    ".": {
      "types": "./dist/bootstrap.d.ts",
      "import": "./dist/bootstrap.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node dist/bootstrap.js"
  },
  "dependencies": {
    "@vena/shared": "workspace:*",
    "@vena/providers": "workspace:*",
    "@vena/core": "workspace:*",
    "@vena/gateway": "workspace:*",
    "@vena/channels": "workspace:*",
    "@vena/semantic-memory": "workspace:*",
    "@vena/voice": "workspace:*",
    "@vena/agents": "workspace:*",
    "@vena/skills": "workspace:*",
    "@vena/computer": "workspace:*",
    "@vena/integrations": "workspace:*",
    "commander": "^13.1.0",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "typescript": "~5.7.3",
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9"
  }
}
